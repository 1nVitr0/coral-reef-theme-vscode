            "settings": {
                "foreground": "#d9c230",
                "fontStyle": null
            }
        },
        {
            "name": "JSDoc-style type brackets",
            "scope": [
                "comment.block.documentation entity.name.type punctuation.definition.bracket"
            ],
            "settings": {
                "foreground": "#14d7bd",
                "fontStyle": null
            }
        },
        {
            "name": "JSDoc-style comment parameters",
            "scope": [
                "comment.block.documentation variable"
            ],
            "settings": {
                "foreground": "#ebebeb",
                "fontStyle": null
            }
        },
        {
            "name": "Constants",
            "scope": [
                "constant"
            ],
            "settings": {
                "foreground": "#b982f7"
            }
        },
        {
            "name": "Language Constants",
            "scope": [
                "constant.language"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
