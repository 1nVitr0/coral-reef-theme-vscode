                "foreground": "#d9c230"
            }
        },
        {
            "name": "Support classes / types",
            "scope": [
                "support.class",
                "support.type"
            ],
            "settings": {
                "foreground": "#d9c230",
                "fontStyle": "bold"
            }
        },
        {
