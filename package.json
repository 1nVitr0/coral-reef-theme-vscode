{
  "name": "theme-coral-reef",
  "displayName": "Coral Reef",
  "description": "Colorful theme loosely based on the Rainglow Vision Theme.",
  "icon": "resources/icon.png",
  "publisher": "1nVitr0",
  "repository": {
    "url": "https://github.com/INVITR0/coral-reef-theme-vscode"
  },
  "version": "1.5.0",
  "engines": {
    "vscode": "^1.13.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Coral Reef",
        "uiTheme": "vs-dark",
        "path": "./theme/coral-reef.json"
      },
      {
        "label": "Coral Reef Bright",
        "uiTheme": "vs-dark",
        "path": "./theme/coral-reef-bright.json"
      },
      {
        "label": "Coral Reef Muted",
        "uiTheme": "vs-dark",
        "path": "./theme/coral-reef-muted.json"
      },
      {
        "label": "Coral Reef Flat (no font styles)",
        "uiTheme": "vs-dark",
        "path": "./theme/coral-reef-bright-flat.json"
      },
      {
        "label": "Coral Reef Bright Flat",
        "uiTheme": "vs-dark",
        "path": "./theme/coral-reef-bright.json"
      },
      {
        "label": "Coral Reef Muted Flat (no font styles)",
        "uiTheme": "vs-dark",
        "path": "./theme/coral-reef-muted-flat.json"
      }
    ]
  },
  "scripts": {
    "build": "ts-node-dev src/scripts/build.ts",
    "build:watch": "ts-node-dev --respawn --watch src/coral-reef.yml src/scripts/build.ts",
    "lint": "ts-node src/scripts/lint.ts"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.1",
    "@semantic-release/git": "^10.0.1",
    "@types/node": "^18.0.0",
    "@types/tinycolor2": "^1.4.3",
    "js-yaml": "^4.1.0",
    "semantic-release": "^21.0.1",
    "semantic-release-vsce": "^5.0.17",
    "tinycolor2": "^1.4.2",
    "ts-node-dev": "^2.0.0",
    "type-fest": "^2.13.1"
  }
}
